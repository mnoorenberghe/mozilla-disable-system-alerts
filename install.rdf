<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>disable-system-alerts@matthew.noorenberghe.com</em:id>
    <em:version>0.9.2</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>

    <em:name>Disable System Alerts</em:name>
    <em:description>Unregister system alert service implementations to fallback to XUL alerts/notifications.</em:description>
    <em:creator>MattN</em:creator>

    <em:targetApplication>
      <Description>
        <!-- Firefox -->
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>22.0</em:minVersion>
        <em:maxVersion>43.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:targetApplication>
      <Description>
        <!-- Mobile Native -->
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>22.0</em:minVersion>
        <em:maxVersion>43.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
